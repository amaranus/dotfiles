! xft fonts  
! ------------------------------------------------------------------

Xft.dpi:       96
Xft.rgba:      rgb
Xft.lcdfilter: lcddefault
Xft.hintstyle: hintfull
Xft.hinting:   true
Xft.autohint:  true 
Xft.antialias: true

! XTerm Ayarlari
! ------------------------------------------------------------------

*faceName: JetBrainsMonoNL Nerd Font Mono
*faceSize: 13
*.vt100.selectToClipboard: true
*.vt100.metaSendsEscape: true
*decTerminalID: vt340
*numColorRegisters: 256
XTerm*colorMode: true
XTerm*defaultDepth: 24
XTerm*customization: true
XTerm*termName: xterm-256color

! Wal Renk kodlari buradan baslar
! ------------------------------------------------------------------

! X colors.
! Generated by 'wal'
*foreground:        #bed5d4
*background:        #353535
*.foreground:       #bed5d4
*.background:       #2e3440
emacs*foreground:   #bed5d4
emacs*background:   #2e3440
URxvt*foreground:   #bed5d4
XTerm*foreground:   #bed5d4
UXTerm*foreground:  #bed5d4
URxvt*background:   rgba:0000/0000/0200/c800
!URxvt*background:   [90]#000000
XTerm*background:   #353535
UXTerm*background:  #2e3440
URxvt*cursorColor:  #bed5d4
XTerm*cursorColor:  #bed5d4
UXTerm*cursorColor: #bed5d4
URxvt*borderColor:  [100]#2e3440

! Colors 0-15.
*.color0: #2e3440
*color0:  #2e3440
*.color1: #607C7F
*color1:  #607C7F
*.color2: #627D81
*color2:  #627D81
*.color3: #709194
*color3:  #709194
*.color4: #7A9FA0
*color4:  #7A9FA0
*.color5: #7DA3A4
*color5:  #7DA3A4
*.color6: #8CB8B7
*color6:  #8CB8B7
*.color7: #bed5d4
*color7:  #bed5d4
*.color8: #859594
*color8:  #859594
*.color9: #607C7F
*color9:  #607C7F
*.color10: #627D81
*color10:  #627D81
*.color11: #709194
*color11:  #709194
*.color12: #7A9FA0
*color12:  #7A9FA0
*.color13: #7DA3A4
*color13:  #7DA3A4
*.color14: #8CB8B7
