! Black color that will not be affected by bold highlighting.
*.color66: #2e3440
*color66:  #2e3440

! Xclock colors.
XClock*foreground: #bed5d4
XClock*background: #2e3440
XClock*majorColor:  rgba:be/d5/d4/ff
XClock*minorColor:  rgba:be/d5/d4/ff
XClock*hourColor:   rgba:be/d5/d4/ff
XClock*minuteColor: rgba:be/d5/d4/ff
XClock*secondColor: rgba:be/d5/d4/ff

! Set depth to make transparency work.
URxvt*depth: 32
